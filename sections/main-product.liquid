 <script>
  document.addEventListener('DOMContentLoaded', function () {
    // Only run on product page
    if (!window.location.pathname.includes('/products/')) return;

    document.querySelectorAll('.quantity').forEach(quantityWrapper => {
      const input = quantityWrapper.querySelector('.quantity__input');
      const minusBtn = quantityWrapper.querySelector('button[name="minus"]');
      const minQty = parseInt(input.dataset.min || 1);

      // Tooltip setup
      const tooltip = document.createElement('div');
      tooltip.classList.add('quantity-tooltip');
      tooltip.innerHTML = '<span class="warning-icon">⚠️</span> This item has a minimum of ' + minQty;
      tooltip.style.display = 'none';

      quantityWrapper.style.position = 'relative';
      quantityWrapper.appendChild(tooltip);

      // Handle minus click
      minusBtn.addEventListener('click', (e) => {
        let currentQty = parseInt(input.value);

        if (currentQty <= minQty) {
          // Block decrement and show warning
          tooltip.style.display = 'block';
          e.preventDefault(); // Stops form or any other default behavior
          return;
        }

       
        tooltip.style.display = 'none';
      });

      // Hide tooltip on outside click
      document.addEventListener('click', function (event) {
        if (!quantityWrapper.contains(event.target)) {
          tooltip.style.display = 'none';
        }
      });
    });
  });



     function initVariantListeners() {
    const quantityInput = document.querySelector(".product-form__quantity .quantity");
    const fieldset = document.querySelector('.product_right_col fieldset');
    if (!fieldset) return;

    // Avoid adding multiple listeners
    if (fieldset.dataset.listenerAttached === 'true') return;
    fieldset.dataset.listenerAttached = 'true';

    fieldset.addEventListener('change', function (e) {
      const target = e.target;
      if (target.matches('input[type="radio"]')) {
        if (target.checked) {
          console.log('Selected size:', target.value);

          const selectedSpan = document.querySelector('legend span[data-selected-value]');
          if (selectedSpan) {
            selectedSpan.textContent = target.value;
          }
        }

        if (target.disabled || target.classList.contains('disabled')) {
          quantityInput?.classList.add('disabled');
        } else {
          quantityInput?.classList.remove('disabled');
        }
      }
    });
  }

  // On initial page load
  document.addEventListener('DOMContentLoaded', () => {
    initVariantListeners();

    // MutationObserver watches for dynamic changes
    const observer = new MutationObserver(() => {
      initVariantListeners();
    });

    observer.observe(document.body, {
      childList: true,
      subtree: true,
    });

    // Update Shop Pay Installments UI
    window.ShopPayInstallments?.UI.update();
  });
   
</script>






